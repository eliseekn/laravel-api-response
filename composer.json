{
    "name": "eliseekn/laravel-api-response",
    "description": "Send JSON response from your API.",
    "keywords": [
        "eliseekn",
        "laravel-api-response"
    ],
    "homepage": "https://github.com/eliseekn/laravel-api-response",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "N'Guessan Kouadio Elis√©e",
            "email": "eliseekn@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Eliseekn\\LaravelApiResponse\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Eliseekn\\LaravelApiResponse\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "aliases": {
                "LaravelApiResponse": "Eliseekn\\LaravelApiResponse\\LaravelApiResponseFacade"
            }
        }
    }
}
